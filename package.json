{
    "name": "@filiphsandstrom/openfactorio",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --watch src -e ts,js,json --exec \"ts-node ./examples/client.ts\"",
        "build": "npm-run-all build:*",
        "build:typescript": "tsc",
        "test": "npm-run-all test:*",
        "test:jest": "jest --passWithNoTests",
        "test:prettier": "prettier --config .prettierrc src/**/*.* --check",
        "format:prettier": "prettier --config .prettierrc src/**/*.* --write",
        "watch:test": "jest --passWithNoTests --watchAll"
    },
    "dependencies": {
        "@jsprismarine/jsbinaryutils": "^2.1.8",
        "body-parser": "1.19.0",
        "express": "4.17.1",
        "moment": "2.29.1",
        "node-fetch": "3.0.0",
        "pino": "^6.10.0",
        "pino-logdna-formatter": "1.2.0",
        "xml2js": "0.4.23"
    },
    "devDependencies": {
        "@babel/preset-env": "7.15.6",
        "@babel/preset-typescript": "7.15.0",
        "@types/express": "4.17.13",
        "@types/jest": "27.0.1",
        "@types/node": "14.17.17",
        "@types/node-fetch": "3.0.2",
        "@types/pino": "6.3.11",
        "@types/xml2js": "0.4.9",
        "@babel/eslint-parser": "7.11.0",
        "husky": "7.0.2",
        "jest": "27.2.0",
        "nodemon": "2.0.12",
        "npm-run-all": "4.1.5",
        "pino-pretty": "7.0.0",
        "prettier": "2.4.1",
        "ts-node": "10.2.1",
        "typescript": "4.4.3",
        "yarnhook": "0.5.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run build && npm run test",
            "post-checkout": "yarnhook",
            "post-merge": "yarnhook",
            "post-rewrite": "yarnhook"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/filiphsandstrom/openfactorio.git"
    },
    "author": "",
    "license": "MIT"
}
